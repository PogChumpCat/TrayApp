<UserControl x:Class="TrayApp.UI.UserControls.DeleteCmbElm"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TrayApp.UI.UserControls"
             mc:Ignorable="d" 
              d:DesignHeight="20" d:DesignWidth="120">
    <Grid>
        <ComboBox x:Name="delEL" HorizontalAlignment="Left" Width="100" Margin="0,0,20,0"/>
        <Button x:Name="BtnDel" Content="-" HorizontalAlignment="Right" Width="20" Margin="0,0,0,0" BorderThickness="0,1,1,1" FontFamily="1" BorderBrush="#FFABADB3" Click="BtnDel_Click"/>
    </Grid>
</UserControl>
